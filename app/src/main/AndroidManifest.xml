<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myjobapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/deskguys_icone"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/deskguys_icone_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyJobApp">
        <activity android:name=".activity_location"></activity>
        <activity android:name=".language"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>